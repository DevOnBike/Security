<Project Sdk="Microsoft.NET.Sdk">

	<PropertyGroup>
		<OutputType>Exe</OutputType>
		<TargetFramework>net9.0</TargetFramework>
		<ImplicitUsings>enable</ImplicitUsings>
		<DebugType>embedded</DebugType>
		<RootNamespace>DevOnBike.Security.Benchmarks</RootNamespace>
		<AssemblyName>DevOnBike.Security.Benchmarks</AssemblyName>
		<AllowUnsafeBlocks>true</AllowUnsafeBlocks>
		<IsPackable>false</IsPackable>
	</PropertyGroup>

	<ItemGroup>
		<PackageReference Include="BenchmarkDotNet" Version="0.15.1" />
	</ItemGroup>

	<ItemGroup>
	  <ProjectReference Include="..\Main\Main.csproj" />
	</ItemGroup>

</Project>
